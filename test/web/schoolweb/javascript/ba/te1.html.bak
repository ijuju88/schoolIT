<!doctype html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <title>Document</title>
  <style>
  h3{text-align:center;}
  </style>
 </head>
 <body>

 <h3>[구구단 출력]</h3>
 
 <table cellpadding="5" bgcolor="Green" align="center">
 <script>
	document.write("<tr>");
	for (var c=2;c<=9 ;c++ )
	{
			document.write("<th>");
			document.write(c+"단");
			document.write("</th>");
	}
	document.write("</tr>");
	for (var i=1;i<=9 ;i++ )
	{
		 document.write("<tr>");
		for (var j=2;j<=9 ;j++ )
		{
			document.write("<td bgcolor='Yellow'>");
			document.write(j+"*"+i+"="+j*i);
			document.write("</td>");
		}
		document.write("</tr>");
	}
	 </script>
 </table>

 </body>
</html>
