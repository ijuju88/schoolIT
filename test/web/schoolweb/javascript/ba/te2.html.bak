  <title>Document</title>
  <script>
  function dan(){
	var d=document.getElementById("dan1").value;
	
	document.write("<table border='1'>");
	for (var i=1;i<=9 ;i++ )
	{
		document.write("<tr>");
		document.write("<td>");
		document.write(d+"*"+i+"="+d*i);
		document.write("</td>");
		document.write("</tr>");
	}
		document.write("</table>");
  }
  </script>
