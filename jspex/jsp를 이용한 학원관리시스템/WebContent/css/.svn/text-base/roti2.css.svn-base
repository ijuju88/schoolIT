#maintop{
			height: 20px;
			width: 1200px;
			font-size: 12px;
            font-weight: bold;
            font-family: "Trebuchet MS";
            text-align: right;
            padding-top: 10px;
}

.aa{
	text-decoration: none;
	color: #7D7D7D;
}

#topMenu {
			height: 40px;
			width: 1270px;
}

#topMenu ul li {
						list-style: none;
                        color: white;
                        background-color: #BC2424;
                        float: left;
                        line-height: 40px;
                        vertical-align: middle;
                        text-align: center;
}

#topMenu .menuLink {
                        text-decoration:none;
                        color: white;
                        display: block;
                        width: 300px;
                        font-size: 13px;
                        font-weight: bold;
                        font-family: "Trebuchet MS";
}
#topMenu .menuLink:hover {
                        color: white;
                        background-color: #368AFF;
}

#stu_Menu {
			height: 25px;
			width: 1225px;
			margin-left:40px;
			border-bottom: 2px solid #BC2424;

}

#stu_Menu ul li {
						list-style: none;
                        color: black;
                        float: left;
                        vertical-align: middle;
                 
}

#stu_Menu .menuLink {
                        text-decoration:none;
                        color: black;
                        display: block;
                        width: 100px;
                        font-size: 13px;
                        font-weight: bold;
                        font-family: "Trebuchet MS";
}
#stu_Menu .menuLink:hover {
                        color: black;
}

#stu_joinout{
			height: 1100px;
			width: 1000px;
			margin-top:100px;
			margin-left:200px;		
			font-size: 13px;
            font-weight: bold;
            font-family: "Trebuchet MS";
            
	
}
#stu_joinin{
			height: 500px;
			width: 700px;
			margin:auto;
}
.star{
	color:red;
	
}

.line_one{
	border-bottom: 2px solid; border-bottom-color: #9E9E9E; padding-bottom: 10px;
}
.line_two{
	border-bottom: 1px solid; border-bottom-color: #D4D4D4; padding-bottom: 10px;
}
/* 일반 input */
input.inputbg{
  padding:5px; /* 내부여백 5px */
  width:200px; /* 입력폼의 가로길이 */
  border:1px solid #d0d0d0; /* 외곽선 1px 색상은 #d0d0d0 */
  background:url(../image/input/bgInput.gif) repeat-x; /* 입력폼 배경 지정 x축반복(repeat-x) */
}
/* 주소 input */
input.inputbg2{
  padding:5px; /* 내부여백 5px */
  width:500px; /* 입력폼의 가로길이 */
  border:1px solid #d0d0d0; /* 외곽선 1px 색상은 #d0d0d0 */
  background:url(../image/input/bgInput.gif) repeat-x; /* 입력폼 배경 지정 x축반복(repeat-x) */
}
/* 전화번호 input */
input.inputbg3{
  padding:5px; /* 내부여백 5px */
  width:100px; /* 입력폼의 가로길이 */
  border:1px solid #d0d0d0; /* 외곽선 1px 색상은 #d0d0d0 */
  background:url(../image/input/bgInput.gif) repeat-x; /* 입력폼 배경 지정 x축반복(repeat-x) */
}

