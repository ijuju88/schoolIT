	private String tch_name;
	private String acab;
	private String resp_sub;
	private String tch_hp;
	
	
	public String getTch_no() {
		return tch_no;
	}
	public void setTch_no(String tch_no) {
		this.tch_no = tch_no;
	}
	public String getTch_pass() {
		return tch_pass;
